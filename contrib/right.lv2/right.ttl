<https://ondrahosek.com/uri/lv2/right>
    a                   lv2:Plugin ,
                        lv2:SpatialPlugin ;
    lv2:project         <https://github.com/RavuAlHemio/lv2-right> ;
    doap:name           "Right" ;
    doap:license        <http://opensource.org/licenses/isc> ;
    lv2:optionalFeature lv2:hardRTCapable ;

    lv2:port [
        a           lv2:AudioPort ,
                    lv2:InputPort ;
        lv2:index   0 ;
        lv2:symbol  "left" ;
        lv2:name    "Left"
    ] , [
        a           lv2:AudioPort ,
                    lv2:InputPort ;
        lv2:index   1 ;
        lv2:symbol  "right" ;
        lv2:name    "Right"
    ] , [
        a           lv2:AudioPort ,
                    lv2:OutputPort ;
        lv2:index   2 ;
        lv2:symbol  "out" ;
        lv2:name    "Out"
    ] .
